{
  "expo": {
    "name": "EazyLink",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0e27"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.offgrid.eazylink",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "We need access to your microphone for calls",
        "NSCameraUsageDescription": "Camera access is required by our VoIP provider",
        "NSLocalNetworkUsageDescription": "We need access to your local network for calling",
        "NSBonjourServiceTypes": ["_services._dns-sd._udp"],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0e27"
      },
      "package": "com.offgrid.eazylink"
    },
    "extra": {
      "eas": {
        "projectId": "425a723f-ef3b-430d-9dab-fefb7bc95e4a"
      }
    },
    "owner": "peeti1163"
  }
}
